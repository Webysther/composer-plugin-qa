{
    "name": "folha/composer-plugin-qa",
    "description": "A full featured composer plugin for PHP QA Tools.",
    "keywords": ["php", "qa", "tools"],
    "type": "composer-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Webysther Nunes",
            "email": "webysther@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3",
        "composer-plugin-api": "^1.0"
    },
    "require-dev": {
        "folha/composer-meta-qa": "~1.0.2"
    },
    "suggest": {
        "folha/composer-meta-qa": "Automatic install all QA tools",
        "hirak/prestissimo": "Composer parallel install plugin"
    },
    "autoload": {
        "psr-4": {
          "Webs\\QA\\": "src"
        }
    },
    "extra": {
        "class": "Webs\\QA\\Plugin"
    }
}
